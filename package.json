{
  "name": "bookmark-indexer",
  "displayName": "Bookmark Indexer",
  "version": "0.0.1",
  "description": "A minimal chrome extension for full-text searching of bookmarked website content.",
  "author": "Tejas Shah",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.2",
    "@emotion/react": "^11.13.3",
    "@mozilla/readability": "^0.5.0",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.12.0",
    "asynchronous-emitter": "^0.1.2",
    "cheerio": "^1.0.0",
    "flexsearch": "^0.7.43",
    "html-to-text": "^9.0.5",
    "linkedom": "^0.18.5",
    "p-progress": "^1.0.0",
    "plasmo": "0.89.3",
    "pythonic": "^2.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-highlight": "^0.15.0",
    "react-icons": "^5.3.0",
    "rfdc": "^1.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/rfdc": "^1.1.0",
    "prettier": "3.2.4",
    "stream-browserify": "^3.0.0",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+X"
        },
        "description": "Open the extension main popup"
      }
    },
    "permissions": [
      "scripting",
      "storage",
      "bookmarks",
      "unlimitedStorage",
      "offscreen"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "tejashah88@gmail.com"
      }
    }
  }
}
